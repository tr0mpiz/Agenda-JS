  <script src="https://cdn.jsdelivr.net/npm/fullcalendar@3.10.2/dist/locale/es.js"></script>

    
    <script>
      $(document).ready(function() {
        
        //si existe el id de la tabla, la inicializo
        if($('#dataTable').length)
        {
          $('#dataTable').DataTable({
            "order": [[ 0, "desc" ]],
            // ponelo en español
            "language": {
              "url": "../assets/vendor/libs/datatables/datatables.es.json"
            }
            
          });
        }
        
        //
        

        
        if($('#calendar').length)
        {

          $.ajax({
            url: '/agenda/calendario?json=1',
            type: 'GET',
            dataType: 'json',
            success: function(response){

              let events = response;
              $('#calendar').fullCalendar({
                  header: {
                    left: 'prev,next today',
                    center: 'title',
                    right: 'month,agendaWeek,agendaDay,listWeek'
                  },
                  defaultDate: moment().format('YYYY-MM-DD'),
                  defaultView: 'agendaWeek', // Mostrar la vista por semana por defecto
                  weekends: false, // Oculta los fines de semana
                  // Oculta los días que no sean de lunes a viernes
                  hiddenDays: [0, 6],
                  navLinks: true, // can click day/week names to navigate views
                  buttonIcons: true,
                  weekNumbers: false,
                  editable: true,
                  eventLimit: true,
                  events: events,
                  locale: 'es',
                  dayClick: function (date, jsEvent, view) {
                     // alert('Has hecho click en: '+ date.format());
                  }, 
                  eventClick: function (calEvent, jsEvent, view) {
                      $('#event-start').val(calEvent.start.format('DD-MM-YYYY HH:mm'));
                      $('#event-end').val(calEvent.end.format('DD-MM-YYYY HH:mm'));
                      $('#modal-title').text(calEvent.title);
                      $('#event-title').val(calEvent.title);
                      $('#event-description').html(calEvent.description);
                      $('#modal-event').modal('show');
                      // hace un modal(show) pero con alguna animacion css
                      
                  }, 
                  

                });
                
            
            },
            //maneja los errores con estados http imprimilo con swall
            error: function(xhr, status, error){
              console.log(xhr);
              console.log(status);
              console.log(error);
            }
          });
          
        }
       
      });
    </script>

