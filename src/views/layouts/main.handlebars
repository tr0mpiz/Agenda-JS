    <script>
      $(document).ready(function() {

        //crea una peticion para recuperar las citas de mañana y pasado mañana y mostralas en el ul que tiene el id recordatorios y la cantidad de recordatorios en el span con el id cantidadRecordatorios
        $.ajax({
          url: '/agenda/recordatorios',
          type: 'GET',
          dataType: 'json',
          success: function(data) {
            const recomendaciones = 'Recorda que tenés que venir con ropa cómoda y calzado de running/deportivo. Saludos.';
            //recorre el data y lo muestra en el ul con el id recordatorios\
            console.log(data);
            $.each(data, function(index, val) {
              $('#recordatorios').append('<li class="list-group-item d-flex align-items-center justify-content-between"><div class="text-small"><strong class="d-block">'+val.nombre+'</strong><span class="d-block text-muted">'+val.fecha_cita+' - '+val.hora+'</span></div><div class="ms-2 text-end"><a href="https://api.whatsapp.com/send?phone='+val.contacto_paciente+'&text=Hola '+val.nombre+' este mensaje es un recordatorio para tu cita pactada para el dia '+val.fecha_cita+' hora '+val.hora+'. '+recomendaciones+' ¡Nos vemos pronto!" target="_blank" class="btn btn-sm btn-primary">Enviar</a></div></li>');
            });
            //muestra la cantidad de recordatorios en el span con el id cantidadRecordatorios
            $('#cantidadrecordatorios').html(data.length);
            
          },
          error: function(data) {
            console.log(data);
          }
        });

        
        //si existe el id de la tabla, la inicializo
        if($('#dataTable').length)
        {
          if($('.agendacontenedor').length)
          {
            $('#dataTable').DataTable({
            //ordenar por posicion 1,2
            "order": [[ 1, "asc" ],[ 2, "asc" ]],
            //sacale el ordenamiento cuando esta en la url tenga /agenda
            //"ordering": false,

            // ponelo en español
            "language": {
              "url": "../assets/vendor/libs/datatables/datatables.es.json"
            }
            
          });
          }else{
            $('#dataTable').DataTable({
            //ordenar por posicion 1,2
            //"order": [[ 1, "asc" ]],
            //sacale el ordenamiento cuando esta en la url tenga /agenda
            //"ordering": false,

            // ponelo en español
            "language": {
              "url": "../assets/vendor/libs/datatables/datatables.es.json"
            }
            
          });
          }
          
        }
        
        //
        

        
        if($('#calendar').length)
        {
          

          document.addEventListener('paste', function(event) {

             const inputFieldsmodalmodal = document.querySelectorAll('#modal-event');
              inputFieldsmodalmodal.forEach(function(inputField) {
                  // Obtener el texto pegado
                  const clipboardData = event.clipboardData || window.clipboardData;
                  const pastedText = clipboardData.getData('text');

                  // Obtener el nombre del campo de entrada
                  const fieldName = inputField.id;
                  
                  // Asignar el texto pegado al campo de entrada correspondiente
                  switch (fieldName) {
                    case 'dni_paciente':
                      document.getElementById('dni_paciente').value = pastedText.split(',')[0];
                      //darle evento change para que busque el paciente
                      document.getElementById('dni_paciente').dispatchEvent(new Event('change'));
                      
                      break;
                    case 'nombre_paciente':
                    document.getElementById('nombre_paciente').value = pastedText.split(',')[1];
                      break;
                    case 'apellido_paciente':
                    document.getElementById('apellido_paciente').value = pastedText.split(',')[2];
                      break;
                    case 'contacto_paciente':
                    document.getElementById('contacto_paciente').value = pastedText.split(',')[3];
                      break;
                    default:
                      break;
                  }
                  

              });
            
            //alert('pegado')
           
        
            
            // Asignar el texto pegado al campo de entrada correspondiente 5678,nicolas,piersanti,1122529738
            

            // Prevenir la acción de pegado predeterminada
            event.preventDefault();
          });
        


          $.ajax({
            url: '/agenda/calendario?json=1',
            type: 'GET',
            dataType: 'json',
            success: function(response){

              let events = response;
              $('#calendar').fullCalendar({
                  header: {
                    left: 'prev,next today',
                    center: 'title',
                    right: 'month,agendaWeek,agendaDay,listWeek'
                  },
                  defaultDate: moment().format('YYYY-MM-DD'),
                  defaultView: 'agendaWeek', // Mostrar la vista por semana por defecto
                  weekends: false, // Oculta los fines de semana
                  // Oculta los días que no sean de lunes a viernes
                  hiddenDays: [0, 6],
                  navLinks: true, // can click day/week names to navigate views
                  buttonIcons: true,
                  weekNumbers: false,
                  editable: true,
                  eventLimit: true,
                  events: events,
                  locale: 'es',
                  //mostra de otro color el dia de hoy
                  dayRender: function (date, cell) {
                    if (date.isSame(moment(), 'day')) {
                        cell.css("background-color", "#ff9f89");
                    }
                  },
                  dayClick: function (date, jsEvent, view) {

                    $('#fecha_cita').val(date.format('YYYY-MM-DD HH:mm:ss'));
                    $('#proxima_cita').val(date.format('YYYY-MM-DD HH:mm:ss'));
                    $('#modal-title-add').text('Agregar Cita - '+date.format('DD-MM-YYYY HH:mm:ss'));
                    $('#dni_paciente').text('');
                    $('#event-description-add').html("");
                    $('#dni_paciente').val('');
                    $('#nombre_paciente').val('');
                    $('#apellido_paciente').val('');
                    $('#contacto_paciente').val('');
                    $('#comentario_cita').val('');
                    $('#id_agenda').val(0);
                    $('#modal-event').modal('show');

                    //eliminar la clase btn-warninr al boton
                   
                    let form = document.getElementById('add-event');
                    form.action = '/agenda/alta';
                    let submitButton = form.querySelector('button[type="submit"]');
                     submitButton.classList.add('btn-primary');
                    //pone la clase btn-primary
                    submitButton.classList.remove('btn-danger');
                    submitButton.textContent = 'Guardar Cita';
                    
                  }, 
                  eventDrop: function(event,dayDelta,minuteDelta,allDay,revertFunc) {

                      let fecha_cita = event.start.format('YYYY-MM-DD HH:mm:ss');
                      let id = event.id;
                      // crea la variable fecha_cita_fin que sea igual a fecha_cita + 30 minutos
                     
                      //console.log(fecha_cita);
                      
                      console.log(fecha_cita);
                      
                      let url = '/agenda/alta?id='+id;
                      let data = {
                        id_agenda: id,
                        fecha_cita: fecha_cita,
                      };
                      $.ajax({
                        url: url,
                        type: 'POST',
                        dataType: 'json',
                        data: data,
                        success: function(response){
                            
                        }
                      });
                      var title = event.title;
                      var message = 'La cita se movio a la fecha '+fecha_cita;
                      var duration = 5000; // 5 segundos
                      
                      showBootstrapToast(title, message, duration);


                  },
                  eventClick: function (calEvent, jsEvent, view) {
                      console.log(calEvent);

                      $('#fecha_cita').val(calEvent.start.format('YYYY-MM-DD HH:mm:ss'));
                      $('#proxima_cita').val(calEvent.end.format('YYYY-MM-DD HH:mm:ss'));
                      $('#modal-title-add').text(calEvent.title);
                      const id = calEvent.id;
                      const url = '/agenda?id='+id;
                      $.ajax({
                        url: url,
                        type: 'GET',
                        dataType: 'json',
                        success: function(response){
                          
